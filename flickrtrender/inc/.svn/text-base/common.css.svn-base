@CHARSET "ISO-8859-1";

body{font:13px/1.231 arial,helvetica,clean,sans-serif;*font-size:small;*font:x-small;}
select,input,button,textarea,button,pre{font:99% arial,helvetica,clean,sans-serif;}
table{font-size:inherit;font:100%;}
code,kbd,samp,tt{font-family:monospace;*font-size:108%;line-height:100%;}

