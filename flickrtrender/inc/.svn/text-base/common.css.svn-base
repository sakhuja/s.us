th, .editorialsubmit{
background: #D8D8DA url(http://yui.yahooapis.com/2.6.0/build/assets/skins/sam/sprite.png) repeat-x scroll 0 0;
}

tr, thead, th, .editorialsubmit {
left:-1500px;
position: absoute;
}

.normalButtton{
background:transparent url(http://yui.yahooapis.com/2.6.0/build/assets/skins/sam/sprite.png) repeat-x scroll 0 0;
border:1px solid #666666;
color:#000000;
font-weight:bold;
text-decoration:none;
padding: 1px 18px;
width: 100%;
white-space: nowrap;
-moz-border-radius:5px;
-webkit-border-radius:5px;
}

.highlightedButtton{
background:transparent url(http://l.yimg.com/a/i/eu/sch/smd/sprite_btn_grd.png) repeat-x scroll 0 0;
border:1px solid #666666;
color:#000000;
font-weight:bold;
text-decoration:none;
padding: 1px 10px;
white-space: nowrap;
-moz-border-radius:5px;
-webkit-border-radius:5px;
}

.highlightedButtton:hover, .normalButtton:hover, .thHighlight:hover, .linkButtton:hover{
background:transparent url(http://l.yimg.com/a/i/eu/sch/smd/sprite_btn_grd.png) repeat-x 0 -75px;
}

.editorialsubmit {
border:1px solid #999b9a;color:blue;cursor:pointer;font-weight:bold;height:2.7em;margin-left:2px;padding:3px 8px 5px 8px;text-align:center;vertical-align:top;white-space:nowrap;*margin-top:2px;
}

.border {
border: 1px solid #999b9a;
/*background-color: #FFF5EE; */
background-color: #FFFAFA;
border-width: 1px;
border-spacing: 0px; 
border-style: outset;
border-color: gray;
border-collapse: collapse;
}

#innerInputTable td, #statsTable td, #rulesTable th, #rulesTable td{
border: 1px solid #999b9a;
padding: 8px;
border-width: 1px;
border-spacing: 0px; 
border-style: inset;
border-color: gray;
border-collapse: collapse;
-moz-border-radius: 0px;
}
#rulesTable td{
background-color: #FFFAFA;
}
#rulesTable{
	min-width:800px;
	background-color: #F8F8F8; text-align: center; border: 1px solid navy;
}
.commenttables {
background-color: #FFF5EE;
border-width: 0px;
border-spacing: 0px; 
border-style: inset;
border-color: gray;
border-collapse: collapse;
-moz-border-radius: 0px;
}

.rightarrow{
float: left; 
font-weight: bold; 
font: 15px;
/*background: transparent url(http://www.facebook.com//rsrc.php/zD0UO/hash/4hvlr9i2.gif) no-repeat scroll 0 -145px;*/
}

.downarrow{
float: left; 
font-weight: bold; 
font: 15px;
}

.Header {  height:25px; background-color:black; color:White; 
           font- size:medium; padding-top:10px; margin-top:-20px;
           margin-left:-10px; background-image:url(http://rightArrow.gif);
           background-repeat:no-repeat; background-position:8% 55%; }

.Header2 { background-color:Gray; color:Black;
           background-image:url(http://downArrow.gif); background-repeat:no-repeat;
           background-position:8% 55%;}
           
.tableheader{
text-align: left; 
border: 1px solid navy; 
width:100%;
}
.thHighlight{
padding: 5px;
border: 1px solid #999b9a; 
cursor: pointer;
text-align: center;
}

.linkButtton{
background:transparent url(http://yui.yahooapis.com/2.6.0/build/assets/skins/sam/sprite.png) repeat-x scroll 0 0;
border:1px solid #999b9a;
color:blue;
padding: 3px 18px;
width: 100%;
white-space: nowrap;
-moz-border-radius:5px;
-webkit-border-radius:5px;
}

.smo-required, .smo-required * {
color:#FF0000;
margin:0;
padding:0;
}
div.smo-metadata p {
color:#999999;
padding:0.25em;
}
div.smo-metadata dt {
color:#999999;
font-size:11px;
line-height:1.5em;
padding:0.25em;
text-align:right;
}
div.smo-metadata dt a {
color:#999999;
display:block;
text-decoration:none;
}
div.smo-metadata dt span {
float:left;
font-size:14px;
}
div.smo-metadata dd {
background-color:#E7E7E7;
color:#333333;
padding:0.25em;
}
div.smo-metadata dd * {
color:#333333;
}
div.smo-metadata dd span {
display:block;
margin-top:0.5em;
}
div.smo-metadata dd ul {
margin-left:30px;
}
div.smo-metadata dd li {
list-style-type:disc;
margin:0.2em 0;
}
div.smo-metadata h3 {
font-size:14px;
text-align:center;
}
